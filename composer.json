{
    "name": "acelaya/zf2-acqrcode",
    "description": "QR Code generation module for Zend Framework 2 based on Endroid QR code",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Celaya AlastruÃ©",
            "email": "alejandro@alejandrocelaya.com",
            "homepage": "https://www.alejandrocelaya.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "zf2",
        "module",
        "qr",
        "php",
        "endroid"
    ],
    "homepage": "https://github.com/acelaya/ZF2-AcQrCode",
    "require": {
        "php" : "^7.0 | ^8.0",
        "endroid/qrcode": "^1.6",
        "laminas/laminas-mvc": ">=3.0",
        "laminas/laminas-router": ">=3.0",
        "laminas/laminas-servicemanager": ">=3.0",
        "laminas/laminas-view": ">=2.8",
        "laminas/laminas-modulemanager": ">=2.7",
        "laminas/laminas-http": ">=2.6",
        "laminas/laminas-eventmanager": ">=3.0",
        "laminas/laminas-stdlib": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": ">=2.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Acelaya\\QrCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acelaya\\QrCode\\Test\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "module": "Acelaya\\QrCode",
            "config-provider": "Acelaya\\QrCode\\ConfigProvider"
        }
    }
}
